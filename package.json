{
  "name": "@balena/jellyfish-plugin-front",
  "description": "Front Jellyfish Plugin",
  "version": "1.3.70",
  "repository": {
    "type": "git",
    "url": "https://github.com/product-os/jellyfish-plugin-front"
  },
  "engines": {
    "node": ">=14.2.0"
  },
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build/"
  ],
  "directories": {
    "lib": "lib"
  },
  "deplint": {
    "files": [
      "lib/**/*.{ts}"
    ],
    "modules": [
      "lib"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "balena-lint --fix"
    ]
  },
  "scripts": {
    "clean": "rimraf build",
    "build": "npm run clean && tsc -p tsconfig.build.json",
    "lint": "balena-lint lib test && npm run check && deplint && depcheck --ignore-bin-package --ignores=@types/jest,typedoc",
    "lint:fix": "balena-lint --fix lib test",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "jest test/unit",
    "test:mirror": "npx jest --runInBand --bail test/integration/front-mirror.spec.ts",
    "test:translate": "npx jest --runInBand --bail test/integration/front-translate.spec.ts",
    "test:compose": "docker build -t balena/jellyfish-sut:latest . && docker-compose -f docker-compose.test.yml -f docker-compose.yml up --exit-code-from=sut",
    "doc": "typedoc lib/ && touch docs/.nojekyll",
    "prepack": "npm run build",
    "check": "jellycheck",
    "compose": "docker-compose up"
  },
  "author": "Balena.io. <hello@balena.io>",
  "license": "AGPL-3.0",
  "dependencies": {
    "@balena/jellyfish-action-library": "^15.1.128",
    "@balena/jellyfish-assert": "^1.1.82",
    "@balena/jellyfish-environment": "^5.1.12",
    "@balena/jellyfish-plugin-base": "^2.1.287",
    "axios": "^0.24.0",
    "bluebird": "^3.7.2",
    "fast-json-patch": "^3.1.0",
    "front-sdk": "^0.8.1",
    "intercom-client": "^2.11.2",
    "lodash": "^4.17.21",
    "lru-cache": "^6.0.0",
    "marked": "^2.1.3",
    "native-url": "^0.3.4"
  },
  "devDependencies": {
    "@balena/jellycheck": "^1.0.0",
    "@balena/jellyfish-config": "^1.5.4",
    "@balena/jellyfish-core": "^8.1.1",
    "@balena/jellyfish-plugin-default": "^21.1.3",
    "@balena/jellyfish-plugin-product-os": "^2.7.0",
    "@balena/jellyfish-sync": "^6.1.5",
    "@balena/jellyfish-test-harness": "^8.1.4",
    "@balena/jellyfish-types": "^1.2.2",
    "@balena/lint": "^6.1.1",
    "@types/bluebird": "^3.5.36",
    "@types/intercom-client": "^2.11.11",
    "@types/jest": "^27.0.2",
    "@types/lodash": "^4.14.176",
    "@types/lru-cache": "^5.1.1",
    "@types/marked": "^2.0.5",
    "@types/uuid": "^8.3.1",
    "date-fns": "^2.25.0",
    "depcheck": "^1.4.2",
    "deplint": "^1.1.3",
    "form-data": "^4.0.0",
    "jest": "^27.3.1",
    "lint-staged": "^11.2.6",
    "nock": "^13.1.4",
    "rimraf": "^3.0.2",
    "simple-git-hooks": "^2.7.0",
    "ts-jest": "^27.0.7",
    "typedoc": "^0.22.7",
    "typescript": "^4.4.4"
  },
  "peerDependencies": {
    "@balena/jellyfish-core": "^8.1.1",
    "@balena/jellyfish-plugin-default": "^21.1.3",
    "@balena/jellyfish-sync": "^6.1.5"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "versionist": {
    "publishedAt": "2021-11-21T11:31:00.271Z"
  }
}
