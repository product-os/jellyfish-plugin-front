{
  "name": "@balena/jellyfish-plugin-front",
  "description": "Front Jellyfish Plugin",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/product-os/jellyfish-plugin-front"
  },
  "engines": {
    "node": ">=14.2.0"
  },
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build/"
  ],
  "directories": {
    "lib": "lib"
  },
  "deplint": {
    "files": [
      "lib/**/*.{ts}"
    ],
    "modules": [
      "lib"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "balena-lint --fix"
    ]
  },
  "scripts": {
    "clean": "rimraf build",
    "build": "npm run clean && tsc -p tsconfig.build.json",
    "lint": "balena-lint lib test && npm run check && deplint && depcheck --ignore-bin-package --ignores=@types/jest",
    "lint-fix": "balena-lint --fix lib test",
    "unit": "jest test/unit",
    "mirror": "npx jest --runInBand --bail test/integration/front-mirror.spec.ts",
    "translate": "npx jest --runInBand --bail test/integration/front-translate.spec.ts",
    "test": "npm run lint && npm run unit",
    "doc": "typedoc lib/ && touch docs/.nojekyll",
    "prepack": "npm run build",
    "check": "jellycheck"
  },
  "author": "Balena.io. <hello@balena.io>",
  "license": "UNLICENSED",
  "dependencies": {
    "@balena/jellyfish-action-library": "^15.0.1",
    "@balena/jellyfish-assert": "^1.1.55",
    "@balena/jellyfish-environment": "^4.3.5",
    "@balena/jellyfish-plugin-base": "^2.1.175",
    "axios": "^0.21.1",
    "bluebird": "^3.7.2",
    "fast-json-patch": "^3.0.0-1",
    "front-sdk": "^0.8.1",
    "intercom-client": "^2.11.2",
    "lodash": "^4.17.21",
    "lru-cache": "^6.0.0",
    "marked": "^2.1.3",
    "native-url": "^0.3.4"
  },
  "devDependencies": {
    "@balena/jellycheck": "^0.1.1",
    "@balena/jellyfish-config": "^1.3.4",
    "@balena/jellyfish-core": "^4.0.21",
    "@balena/jellyfish-plugin-default": "^17.0.0",
    "@balena/jellyfish-sync": "^6.0.267",
    "@balena/jellyfish-test-harness": "^5.1.0",
    "@balena/jellyfish-types": "^0.8.0",
    "@balena/lint": "^6.1.1",
    "@types/bluebird": "^3.5.35",
    "@types/intercom-client": "^2.11.10",
    "@types/jest": "^26.0.23",
    "@types/lodash": "^4.14.170",
    "@types/lru-cache": "^5.1.0",
    "@types/marked": "^2.0.3",
    "@types/sinon": "^10.0.2",
    "@types/uuid": "^8.3.0",
    "depcheck": "^1.4.2",
    "deplint": "^1.1.3",
    "form-data": "^4.0.0",
    "jest": "^27.0.6",
    "lint-staged": "^11.0.0",
    "nock": "^13.1.0",
    "rimraf": "^3.0.2",
    "simple-git-hooks": "^2.5.1",
    "sinon": "^11.1.1",
    "ts-jest": "^27.0.3",
    "typedoc": "^0.21.2",
    "typescript": "^4.3.5"
  },
  "peerDependencies": {
    "@balena/jellyfish-core": "^4.0.21",
    "@balena/jellyfish-plugin-default": "^17.0.0",
    "@balena/jellyfish-sync": "^6.0.267"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  }
}
